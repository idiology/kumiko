<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>KUMIKO</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/assets/kumiko.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/assets/kumiko.css" asp-append-version="true" />
    </environment>
</head>
<body>    
    @RenderBody()   
    <environment include="Development">
        <script type="text/javascript" src="~/assets/kumiko.js"></script>
    </environment>
    <environment exclude="Development">
        <script type="text/javascript" src="~/assets/kumiko.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", false)
</body>
</html>